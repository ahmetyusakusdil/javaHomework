package thirdDaySecondHomework;

public class Instructor extends User{

	private int id;
	private String givenCourse;
	private String givenHomework;
	private String about;
	
	public Instructor () {
		
	}

	public Instructor(int id ,int userId,String givenCourse,
			String givenHomework, String about) {
		this.id=id;
		this.givenCourse = givenCourse;
		this.givenHomework = givenHomework;
		this.about = about;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getGivenCourse() {
		return givenCourse;
	}

	public void setGivenCourse(String givenCourse) {
		this.givenCourse = givenCourse;
	}

	public String getGivenHomework() {
		return givenHomework;
	}

	public void setGivenHomework(String givenHomework) {
		this.givenHomework = givenHomework;
	}

	public String getAbout() {
		return about;
	}

	public void setAbout(String about) {
		this.about = about;
	}


	
	
		
	
	
}
