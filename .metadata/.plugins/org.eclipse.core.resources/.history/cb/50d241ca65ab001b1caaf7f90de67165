package thirdDaySecondHomework;

public class StudentManager {

	public void addTakenCourse(Student student) {
		
		System.out.println(student.getTakenCourse() + " Kursu alindi.");
	}
	
	public void addDoneHomeword(Student student) {
		System.out.println(student.getDoneHomework() + " Ödevi yapildi.");
	}
	
	
}
